        <Button x:Name="btnImport" Content="Import" HorizontalAlignment="Left" Margin="318,13,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.303,-0.282" Width="43" Click="btnImport_Click"/>
        <ListBox x:Name="lstResult" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,101,20,29"/>
